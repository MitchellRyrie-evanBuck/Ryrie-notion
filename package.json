{
  "name": "ryrie-notion",
  "version": "2.0.0",
  "private": true,
  "description": "The perfect starter kit for building beautiful websites with Next.js and Notion.",
  "author": "ryrie <liuxiaowen66621@gmail.com>",
  "repository": "MitchellRyrie-evanBuck/Ryrie-notion",
  "license": "MIT",
  "engines": {
    "node": ">=20",
    "pnpm": ">=9.12.2"
  },
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "deploy": "vercel deploy",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "deps:upgrade": "[ -z $GITHUB_ACTIONS ] && pnpm up -L notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:update on CI'",
    "deps:link": "[ -z $GITHUB_ACTIONS ] && run-s deps:link:* || echo 'Skipping deps:update on CI'",
    "deps:unlink": "[ -z $GITHUB_ACTIONS ] && pnpm add notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:update on CI'",
    "deps:link:notion-types": "pnpm link ../react-notion-x/packages/notion-types",
    "deps:link:notion-utils": "pnpm link ../react-notion-x/packages/notion-utils",
    "deps:link:notion-client": "pnpm link ../react-notion-x/packages/notion-client",
    "deps:link:react-notion-x": "pnpm link ../react-notion-x/packages/react-notion-x",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "test": "run-p test:*",
    "test:lint": "eslint .",
    "test:prettier": "prettier '**/*.{js,jsx,ts,tsx}' --check",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ai-sdk/openai": "1.3.17",
    "@ai-sdk/react": "1.2.9",
    "@fisch0920/use-dark-mode": "^2.4.0",
    "@giscus/react": "3.1.0",
    "@keyvhq/core": "^1.6.26",
    "@keyvhq/redis": "^1.6.28",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@react-icons/all-files": "^4.1.0",
    "@sentry/nextjs": "9.1.0",
    "@tabler/icons-react": "^3.34.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "ai": "4.3.9",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "colorthief": "2.6.0",
    "expiry-map": "^2.0.0",
    "fathom-client": "^3.7.2",
    "framer-motion": "11.18.2",
    "ky": "^1.8.1",
    "lodash.omit": "^4.5.0",
    "lottie-react": "^2.4.1",
    "lqip-modern": "^2.2.1",
    "lucide-react": "^0.469.0",
    "motion": "^11.18.2",
    "next": "^15.3.5",
    "next-auth": "5.0.0-beta.25",
    "notion-client": "^7.4.2",
    "notion-types": "^7.4.2",
    "notion-utils": "^7.4.2",
    "openai": "4.95.1",
    "p-map": "^7.0.3",
    "p-memoize": "^7.1.1",
    "posthog-js": "^1.256.2",
    "prismjs": "^1.30.0",
    "qss": "^3.0.0",
    "raf": "^3.4.1",
    "random": "^5.4.0",
    "react": "^19.1.0",
    "react-body-classname": "^1.3.1",
    "react-dom": "^19.1.0",
    "react-fluid-animation": "^1.0.1",
    "react-hook-form": "^7.60.0",
    "react-icons": "^5.5.0",
    "react-notion-x": "^7.4.2",
    "react-syntax-highlighter": "^15.6.1",
    "react-tweet": "^3.2.2",
    "react-use": "^17.6.0",
    "rss": "^1.2.2",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.74"
  },
  "devDependencies": {
    "@commitlint/cli": "19.7.1",
    "@commitlint/config-conventional": "19.7.1",
    "@fisch0920/eslint-config": "^1.4.0",
    "@next/bundle-analyzer": "^15.3.5",
    "@types/lodash.omit": "^4.5.9",
    "@types/node": "^22.16.0",
    "@types/raf": "^3.4.3",
    "@types/react": "^18.3.23",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.3.5",
    "husky": "9.1.7",
    "lint-staged": "15.4.3",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "overrides": {
    "cacheable-request": {
      "keyv": "npm:@keyvhq/core@~1.6.6"
    }
  },
  "packageManager": "pnpm@9.14.4+sha512.c8180b3fbe4e4bca02c94234717896b5529740a6cbadf19fa78254270403ea2f27d4e1d46a08a0f56c89b63dc8ebfd3ee53326da720273794e6200fcf0d184ab"
}
